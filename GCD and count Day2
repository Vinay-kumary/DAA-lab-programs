#include <stdio.h>
int count=0;
int main()
{
    int n1, n2;
    printf("Enter two positive integers: ");
    scanf("%d %d",&n1,&n2);
    count++;
    while(n1!=n2)
        {
        count++;
        if(n1 > n2)
        {
		
            n1 = n2;
            count++;
    }
        else
        {
            n2 = n1;
            count++;
    }
    count++;
}
count++;
    printf("GCD = %d",n1);
    printf("count = %d",count);
    
    return 0;
}
